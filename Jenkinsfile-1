pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
            sh '''
            ./mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.2:sonar \
            -Dsonar.login=admin \
            -Dsonar.password=ruihuan3
            '''
        }
      }
    }
  }
}
